                <div class="container-fluid text-light footer pt-5 mt-5 wow fadeIn" data-wow-delay="0.1s">
                    <div class="container py-5">
                        <div class="row g-5">
                            <div class="col-lg-3 col-md-6">
                                <h4 class="text-dark mb-3">Menu</h4>
                                <a href="{{ path ('index') }}" class="btn text-dark btn-link">Accueil</a>
                                <a href="{{ path ('service') }}" class="btn text-dark btn-link">Nos Services</a>
                                <a href="{{ path ('about') }}" class="btn text-dark btn-link">A Propos</a>
                                <a href="{{ path ('reservation') }}" class="btn text-dark btn-link">Réservation</a>
                                <a href="{{ path ('contact') }}" class="btn text-dark btn-link">Contact</a>
                            </div>
                            <div class="col-lg-3 col-md-6">
                                <h4 class="text-dark mb-3">Contact</h4>
                                <p class="mb-2 text-dark"><i class="fa fa-map-marker-alt me-3"></i>10 rue des lilas, Créteil, 94000</p>
                                <p class="mb-2 text-dark"><i class="fa fa-phone-alt me-3"></i>+33.1.56.37.84.85</p>
                                <p class="mb-2 text-dark"><i class="fa fa-envelope me-3"></i>gustocoffee@gmail.com</p>
                                <div class="d-flex pt-2">
                                    <a class="btn btn-booking btn-social" href=""><i class="fab fa-twitter"></i></a>
