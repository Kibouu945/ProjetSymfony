      <!-- Navbar & Hero Start -->
            <div class="container-fluid position-relative p-0">
                <nav class="navbar navbar-expand-lg navbar-light px-4 px-lg-5 py-3 py-lg-0">
                    <a href="{{ path ('index') }}" class="navbar-brand p-0">
                        <h1 class="text-primary m-0"><i class=""></i></h1>
                         <img src= "{{asset('img/logogust.png')}}" alt="Logo">
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                        <span class="fa fa-bars"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarCollapse">
                        <div class="navbar-nav ms-auto py-0">
                            <a href="{{ path ('index') }}" class="nav-item nav-link active">Accueil</a>
                            <a href="{{ path ('service') }}" class="nav-item nav-link">Nos Services</a>
                            <a href="{{ path ('about') }}" class="nav-item nav-link">A Propos</a>
                            <a href="{{ path ('reservation') }}" class="nav-item nav-link">Réservation</a>
                            <a href="{{ path ('contact') }}" class="nav-item nav-link">Contact</a>
                        </div>
                        <a href="{{ path ('app_register') }}" class="btn btn-primary rounded-pill py-2 px-4">Inscription</a>
                    </div>
                </nav>

                <div class="container-fluid bg-primary py-5 mb-5 hero-header">
                    <div class="container py-5">
                        <div class="row justify-content-center py-5">
                            <div class="col-lg-10 pt-lg-5 mt-lg-5 text-center">
